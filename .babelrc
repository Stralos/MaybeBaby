{ 
  "presets": [
    "react", 
    "es2015", 
    "stage-0"
  ],
  "plugins": [
    ["react-hot-loader/babel"],
    ["babel-plugin-styled-components", {
        "ssr": true
    }],
    ["module-resolver", {
      "root": ["./app"],
      "alias": {
        "components": "./components",
        "containers": "./containers",
        "utils": "./utils",
        "selectors": "./selectors"
      }
    }]
  ]
}
