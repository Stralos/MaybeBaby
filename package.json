    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "flow-status-webpack-plugin": "^0.1.7",
    "jest": "^21.2.1",
    "jest-styled-components": "^4.9.0",
    "npm-run-all": "^4.1.2",
    "react-test-renderer": "^16.2.0",
    "webpack-node-externals": "^1.6.0"
  }
