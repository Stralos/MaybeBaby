  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Stralos/MaybeBaby/issues"
  },
  "homepage": "https://github.com/Stralos/MaybeBaby#readme",
  "engines": {
    "node": "6.10.2"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-flowtype-errors": "^3.3.6",
    "express": "^4.16.2",
    "flow-babel-webpack-plugin": "^1.1.0",
    "flow-bin": "^0.60.1",
    "flow-typed": "^2.2.3",
    "history": "^4.7.2",
    "nodemon": "^1.12.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "remove-flow-types-loader": "^1.0.0",
    "styled-components": "^2.2.4",
    "webpack": "^3.6.0",
    "webpack-shell-plugin": "^0.5.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.0",
    "babel-plugin-module-resolver": "^2.7.1",
