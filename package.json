{
  "name": "maybe-baby",
  "version": "1.0.0",
  "description": "A React.js + flow + Node.js servers side rendering mini project for learning purposes.",
  "main": "index.js",
  "scripts": {
    "start": "node ./dist/server-bundle.js",
    "build:dev": "webpack -d",
    "watch:dev": "npm-run-all --parallel watch:dev:*",
    "watch:dev:build": "webpack -w",
    "watch:dev:server": "nodemon ./dist/server-bundle.js",
    "update": "flow-typed install",
    "flow": "flow status",
    "test": "jest",
    "lint": "eslint --ext .jsx --ext .js app/",
    "lint:fix": "eslint --ext .jsx --ext .js app/ --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Stralos/MaybeBaby.git"
  },
